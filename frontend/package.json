    "date-fns": "^2.30.0",
    "framer-motion": "^10.18.0",
    "jalaali-js": "^1.1.0",
    "lenis": "^1.3.17",
    "lucide-react": "^0.563.0",
    "next": "14.2.35",
    "postcss": "^8.4.31",
    "qrcode.react": "^4.2.0",
